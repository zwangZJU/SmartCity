    android:layout_height="match_parent"
    android:clickable="true"
    android:background="#f3f3f3"
    >

    <com.wzlab.smartcity.widget.LoadingLayout
        android:id="@+id/loading_layout_repair_log"
        android:layout_width="match_parent"
